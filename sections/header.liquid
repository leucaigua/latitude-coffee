<link rel="stylesheet" href="{{ 'component-loading-overlay.css' | asset_url }}" media="print" onload="this.media='all'">
<script src="{{ 'details-disclosure.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'details-modal.js' | asset_url }}" defer="defer"></script>
<div class="main-navigationbar sticky_header" id="header-sticky">
    <div class="container">
        <div class="navigationbar-row d-flex align-items-center">
            <div class="logo-col">
                <h1>
                    <a href="{{ routes.root_url }}">
                        <img src="{{ section.settings.logo | img_url: 'master' }}" alt="logo" class="header__img">
                    </a>
                </h1>
            </div>
            <div class="menu-items-col">
                <ul class="main-nav">
                    {% for block in section.blocks %}
                        {% assign canonical_url_trim = canonical_url | replace: request.origin %}
                        {% assign block_url_trim = block.settings.link | replace: request.origin %}
                        {% if canonical_url_trim == "/" %}{% assign canonical_url_trim = "" %}{% endif %}
                        {% if block.type == "link-simple" or block.type == "link-linklist" or block.type == "link-4linklists" %}  
                            <li class="menu-lnk{% case block.type %}{% when 'link-linklist' %} has-item{% when 'link-4linklists' %} has-item{% endcase %}">
                                <a class="{{ block.settings.diffclassname }}" href="{% if block.settings.link != blank  %}{{ block.settings.link }} {% else %} javascript:; {% endif %}">
                                    {{ block.settings.title }}
                                </a>

                                <div class="mega-menu__workaround"></div>
                                
                                {% case block.type %}
                                    {% when 'link-linklist' %}
                                    {% if block.settings.links.size > 0 %}
                                    <div class="menu-dropdown second-menu-dropdown">
                                        {%- assign links_small = block.settings.links -%}
                                        <ul id="menu_{{ block.id }}">
                                            {% for link in linklists[links_small].links %}
                                                {%- assign child_list_handle = link.title | handleize -%}
                                                {%- assign link_count = 0 -%}
                                                {% for childlink in linklists[child_list_handle].links %}
                                                    {%- assign link_count = link_count | plus: 1 -%}
                                                {% endfor %}
                                                <li class="{% if link_count > 0 %}with_ul{% endif %}">
                                                    <a href="{{ link.url }}">{{ link.title }}{% if link_count > 0 %}<i class="level_3__trigger megamainhd" data-submenu="submenu_{{ block.id }}-{{ link_count }}"></i>{% endif %}</a>
                                                    {% if link_count > 0 %}
                                                        <ul id="1submenu_{{ block.id }}-{{ link_count }}">
                                                            {% for childlink in linklists[child_list_handle].links %}
                                                                {%- assign child_list_2_handle = childlink.title | handleize -%}
                                                                {%- assign link_count = 0 -%}
                                                                {% for childlink in linklists[child_list_2_handle].links %}
                                                                {%- assign link_count = link_count | plus: 1 -%}
                                                                {% endfor %}                                                                        
                                                                <li class="{% if link_count > 2 %}with_ul2{% endif %}">
                                                                    <a href="{{ childlink.url }}">{{ childlink.title }}{% if link_count > 2 %}<i class="level_3__trigger megamainhd" data-submenu="subsubmenu_{{ block.id }}-{{ link_count }}"></i>{% endif %}</a>
                                                                    {% if link_count > 2 %}
                                                                        <ul id="2subsubmenu_{{ block.id }}-{{ link_count }}">
                                                                        {% for childlink in linklists[child_list_2_handle].links %}
                                                                            <li>
                                                                            <a href="{{ childlink.url }}">{{ childlink.title }}</a>
                                                                            </li>
                                                                        {% endfor %}
                                                                        </ul>
                                                                    {% endif %}
                                                                </li>
                                                            {% endfor %}
                                                        </ul>
                                                    {% endif %}
                                                </li>
                                            {% endfor %}
                                        </ul>
                                    </div>
                                    {% endif %}
                
                                    {% when 'link-4linklists' %}
                                    {% if block.settings.links_1.size > 0 or block.settings.links_2.size > 0 or block.settings.links_3.size > 0 or block.settings.links_4.size > 0 %}
                                        <div class="mega-menu menu-dropdown">
                                            <div class="mega-menu-container container">
                                                <ul class="row" id="menu_{{ block.id }}">
                                                    {% for i in (1..4) %}  
                                                    {% capture list %}links_{{ i }}{% endcapture %}
                                                    {%- assign list = block.settings[list] -%}
                                                    {% capture listtitle %}link_title{{ i }}{% endcapture %}
                                                    {%- assign listtitle = block.settings[listtitle] -%}
                                                        <li class="col-md-2 col-12">                                          
                                                            <ul class="megamenu-list arrow-list" id="menu_{{ block.id }}-{{ i }}">
                                                                <li class="list-title"><span>{{ listtitle }}</span></li>
                                                                {% for link in linklists[list].links %}
                                                                    <li>
                                                                    {% if link.title contains '[new]' %}
                                                                    {%- assign link_text = link.title | replace: '[new]', '' -%}
                                                                    {%- assign link_badge = 'new' -%}
                                                                    {% elsif link.title contains '[hot]' %} 
                                                                    {%- assign link_text = link.title | replace: '[hot]', '' -%}
                                                                    {%- assign link_badge = 'hot' -%}   
                                                                    {% else %}
                                                                    {%- assign link_text = link.title -%}
                                                                    {%- assign link_badge = false -%}   
                                                                    {% endif %}
                                                                    <a href="{{ link.url }}">{{ link_text }}{% if link_badge %}<span class="badge badge_{{ link_badge }}">{{ link_badge }}</span>{% endif %}</a>
                                                                    </li>
                                                                {% endfor %}
                                                            </ul>
                                                        </li>
                                                    {% endfor %}
                                                    {% if block.settings.image != blank %}
                                                        <li class="col-md-3 col-12">
                                                            <a href="{{ block.settings.imageblink }}"> 
                                                                <img loading="lazy" src="{{ block.settings.image | img_url: 'master' }}" alt="menubaner">
                                                            </a>
                                                        </li>
                                                    {% endif %}
                                                </ul>
                                            </div>
                                        </div>
                                    {% endif %}
                                {% endcase %}
                            </li>
                        {% endif %}
                    {% endfor %}
                </ul>
                <ul class="menu-right d-flex justify-content-end align-items-center"> 
                    <li class="menu-lnk">
                        <a href="https://latitudecoffee.com/pages/subscribe-and-save">SUBSCRIBE &amp; SAVE</a>
                    </li>
                    <li class="search-header">
                        <a href="javascript:;" class="search-btn header__icon--search header__icon--summary focus-inset modal__toggle" aria-haspopup="dialog" aria-label="{{ 'general.search.search' | t }}" title="search" id="header-search">
                            <svg xmlns="http://www.w3.org/2000/svg" width="15" height="15" viewBox="0 0 13 13"
                                fill="none">
                                <path fill-rule="evenodd" clip-rule="evenodd"
                                    d="M9.47487 10.5131C8.48031 11.2863 7.23058 11.7466 5.87332 11.7466C2.62957 11.7466 0 9.11706 0 5.87332C0 2.62957 2.62957 0 5.87332 0C9.11706 0 11.7466 2.62957 11.7466 5.87332C11.7466 7.23058 11.2863 8.48031 10.5131 9.47487L12.785 11.7465C13.0717 12.0332 13.0717 12.4981 12.785 12.7848C12.4983 13.0715 12.0334 13.0715 11.7467 12.7848L9.47487 10.5131ZM10.2783 5.87332C10.2783 8.30612 8.30612 10.2783 5.87332 10.2783C3.44051 10.2783 1.46833 8.30612 1.46833 5.87332C1.46833 3.44051 3.44051 1.46833 5.87332 1.46833C8.30612 1.46833 10.2783 3.44051 10.2783 5.87332Z"
                                    fill="#c7ab84"></path>
                            </svg>
                        </a>
                    </li>
                  {%- if shop.customer_accounts_enabled -%}
                    <li class="profile-header">
                      <a href="{% if customer %}{{ routes.account_url }}{% else %}{{ routes.account_login_url }}{% endif %}" class="profile-btn" title="my profile">
                        <svg xmlns="http://www.w3.org/2000/svg" width="15" height="18" viewBox="0 0 15 18" fill="none">
                          <path fill-rule="evenodd" clip-rule="evenodd" d="M7.5 9.81818C4.27834 9.81818 1.66667 12.3824 1.66667 15.5455V17.1818C1.66667 17.6337 1.29357 18 0.833333 18C0.373096 18 0 17.6337 0 17.1818V15.5455C0 11.4786 3.35786 8.18182 7.5 8.18182C11.6421 8.18182 15 11.4786 15 15.5455V17.1818C15 17.6337 14.6269 18 14.1667 18C13.7064 18 13.3333 17.6337 13.3333 17.1818V15.5455C13.3333 12.3824 10.7217 9.81818 7.5 9.81818Z" fill="#c7ab84"/>
