    "43576fc0-7509-439c-8db5-631f65d057cf": {
      "type": "testimonials",
      "blocks": {
        "b0df2ac9-1424-43fb-919c-786aed3e7902": {
          "type": "text",
          "settings": {
            "c_name": "Espresso",
            "proimg_1": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_1": "Monsooned Malabar",
            "title_1": "Great espresso flavor profile, and lower acidity",
            "desc_1": "Latitude Coffee hit another home run with Monsooned Malabar! My better half was having occasional stomach issues, and missing her daily cappuccino treat.",
            "author_1": "Dawson Hovey",
            "designation_1": "Client",
            "img_1": "shopify:\/\/shop_images\/Reviews.png",
            "review_1": "5",
            "proimg_2": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_2": "90 Mile Stretch",
            "title_2": "I'm a huge coffee lover",
            "desc_2": "Nothing compares to the flavor and freshness of Latitude 23.5 coffee. The natural and organic ingredients translate to unparalleled quality, it's like having a coffee shop in my own home!",
            "author_2": "Dina Khrapko",
            "designation_2": "Client",
            "img_2": "shopify:\/\/shop_images\/Reviews.png",
            "review_2": "5",
            "proimg_3": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_3": "Espresso Bella",
            "title_3": "I can’t recommend Latitude 23.5 enough!",
            "desc_3": "I ordered ground coffee over the phone and Holly was so friendly and helpful. They have the best coffee with so many amazing flavors",
            "author_3": "Samantha Thorn",
            "designation_3": "Client",
            "img_3": "shopify:\/\/shop_images\/Reviews.png",
            "review_3": "5",
            "proimg_4": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_4": "Espresso Venezia",
            "title_4": "Great Service!",
            "desc_4": "I have been ordering all my coffee from Latitude 23.5 for years. I prefer to buy organic and that what these folks do. If you combine high quality organic coffee (and tea) with personalized attention, you have an unbeatable match. I've been a happy customer for a long time, and you can be too.",
            "author_4": "Julie Malsin",
            "designation_4": "Client",
            "img_4": "shopify:\/\/shop_images\/Reviews.png",
            "review_4": "5"
          }
        },
        "448e4586-8ea4-4e52-a6f2-486bea576e7c": {
          "type": "text",
          "settings": {
            "c_name": "Blend",
            "proimg_1": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_1": "Breakfast Blend",
            "title_1": "Wonderful coffee roasters!",
            "desc_1": "Very friendly, helpful, informative staff. Delicious variety of coffee beans. Definitely our 'go to 'shop for coffee beans while in the area. Very reasonable prices too. We will continue to order on line and use their shipping options! So glad we found them.",
            "author_1": "Candace Fraser",
            "designation_1": "Client",
            "img_1": "shopify:\/\/shop_images\/Reviews.png",
            "review_1": "5",
            "proimg_2": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_2": "Estate Blend",
            "title_2": "Great coffee!",
            "desc_2": "Relocated from the east coast to Seattle and still order from Latitude. They always make sure we have exactly what we're looking for. For those of us that dont know peaberries from snozzberries; Holly has the expertise to get you the right cup of coffee.",
            "author_2": "Sarah",
            "designation_2": "Client",
            "img_2": "shopify:\/\/shop_images\/Reviews.png",
            "review_2": "5",
            "proimg_3": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_3": "House Blend",
            "title_3": "Wonderful Experience!",
            "desc_3": "We have been ordering coffee from Holly and team for years.  Wonderful experience and the absolute best beans.  We use their subscription service so we never run out of coffee",
            "author_3": "Kellee Kramer",
            "designation_3": "Client",
            "img_3": "shopify:\/\/shop_images\/Reviews.png",
            "review_3": "5",
            "proimg_4": "shopify:\/\/shop_images\/Front_package.png",
            "subtitle_4": "Roast Master's Reserve",
            "title_4": "I highly recommend Latitude 23.5!",
            "desc_4": "I’ve been getting coffee beans from here for the last three years... Outstanding Service, Friendly Staff and most of all they have the perfect bean to fit any palate!",
            "author_4": "Seth Bolyard",
            "designation_4": "Client",
            "img_4": "shopify:\/\/shop_images\/Reviews.png",
            "review_4": "5"
          }
        }
